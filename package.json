{
  "name": "with-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev-frontend": "next dev --port 3000",
    "dev-backend": "vercel dev --listen 3001",
    "dev": "concurrently \"npm run dev-frontend\" \"npm run dev-backend\"",
    "start": "concurrently \"npm run start-frontend\" \"npm run start-backend\"",
    "start-frontend": "next start",
    "start-backend": "vercel dev --listen 3001",
    "build": "next build",
    "export": "next export",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "typescript": "^5"
  }
}
